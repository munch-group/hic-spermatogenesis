  author = {Batra, Sanjit Singh and {Levy-Sakin}, Michal and Robinson, Jacqueline and Guillory, Joseph and Durinck, Steffen and Vilgalys, Tauras P and Kwok, Pui-Yan and Cox, Laura A and Seshagiri, Somasekar and Song, Yun S and Wall, Jeffrey D},
