@misc{open2c2022cooltoolsenablinghighresolution,
  title = {Cooltools: Enabling High-Resolution {{Hi-C}} Analysis in {{Python}}},
  shorttitle = {Cooltools},
  author = {{Open2C} and Abdennur, Nezar and Abraham, Sameer and Fudenberg, Geoffrey and Flyamer, Ilya M. and Galitsyna, Aleksandra A. and Goloborodko, Anton and Imakaev, Maxim and Oksuz, Betul A. and Venev, Sergey V.},
